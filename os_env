export KUBECTL_API_SERVER=10.0.45.126
export FLEETCTL_TUNNEL=$KUBECTL_API_SERVER
export ETCDCTL_ENDPOINT=127.0.0.1:2379
export OS_TENANT_NAME=backend
export OS_AUTH_URL=http://10.0.45.100:5000/v2.0

echo USER:
read OS_USERNAME
export OS_USERNAME=$OS_USERNAME

echo PASSWORD:
read -rs OS_PASSWORD
export OS_PASSWORD=$OS_PASSWORD

ssh -f -nNT -L 8080:127.0.0.1:8080 core@$KUBECTL_API_SERVER

